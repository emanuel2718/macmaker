{
  "breadcrumbs.enabled": false,
  "workbench.editor.showTabs": false,
  "editor.fontSize": 14,
  "editor.formatOnSave": true,
  "editor.hideCursorInOverviewRuler": true,
  "editor.overviewRulerBorder": false,
  "editor.minimap.enabled": false,
  "editor.mouseWheelZoom": true,
  "editor.renderControlCharacters": true,
  "editor.renderWhitespace": "trailing",
  "editor.scrollbar.horizontal": "hidden",
  "editor.scrollbar.horizontalScrollbarSize": 2,
  "editor.scrollbar.vertical": "hidden",
  "editor.scrollbar.verticalScrollbarSize": 2,
  "editor.tabSize": 2,
  "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?",
  "editor.glyphMargin": false,
  "editor.wordBasedSuggestions": false,
  "editor.cursorBlinking": "solid",
  "editor.quickSuggestions": {
    "other": true,
    "comments": false,
    "strings": false
  },
  "editor.renderLineHighlight": "none",
  "diffEditor.renderSideBySide": false,
  "files.exclude": {
    "**/.git": true,
    ".idea": true,
    ".vscode": true,
    "**/__pycache__": true,
    "**/*.pyc": true,
    "node_modules/": true,
    "templates_module/": true,
    "**/.project": true,
    ".mypy_cache/": true,
    "**/.coverage": true
  },
  "files.trimTrailingWhitespace": true,
  "files.trimFinalNewlines": true,
  "workbench.iconTheme": "vscode-icons",
  "workbench.layoutControl.enabled": false,
  "workbench.layoutControl.type": "menu",
  "workbench.statusBar.visible": true,
  "editor.lineNumbers": "off",
  "editor.bracketPairColorization.enabled": true,

  "vim.leader": "<space>",
  "vim.easymotion": true,
  "vim.incsearch": true,
  "vim.hlsearch": true,
  "vim.useSystemClipboard": false,
  "vim.useCtrlKeys": true,
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-f>": false
  },
  "vim.searchHighlightColor": "rgba(150, 255, 255, 0.3)",

  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["leader", "f", "s"],
      "commands": ["workbench.action.files.save"]
    },
    {
      "before": ["leader", "."],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["leader", "s", "p"],
      // find in files
      "commands": ["workbench.action.findInFiles"]
    },
    {
      "before": ["leader", "e"],
      "commands": ["workbench.action.toggleSidebarVisibility"]
    },
    {
      "before": ["leader", "w", "d"],
      "commands": ["workbench.action.closeActiveEditor"]
    },
    {
      "before": ["leader", "w", "v"],
      "commands": ["workbench.action.splitEditor"]
    },
    {
      "before": ["leader", "w", "s"],
      "commands": ["workbench.action.splitEditorDown"]
    },
    {
      "before": ["leader", "w", "l"],
      "commands": ["workbench.action.navigateRight"],
      "when": ["explorerViewletVisible && filesExplorerFocus && !inputFocus"]
    },
    {
      "before": ["leader", "w", "h"],
      "commands": ["workbench.action.navigateLeft"],
      "when": ["explorerViewletVisible && filesExplorerFocus && !inputFocus"]
    },
    {
      "before": ["leader", "w", "k"],
      "commands": ["workbench.action.navigateUp"],
      "when": ["explorerViewletVisible && filesExplorerFocus && !inputFocus"]
    },
    {
      "before": ["leader", "w", "j"],
      "commands": ["workbench.action.navigateDown"],
      "when": ["explorerViewletVisible && filesExplorerFocus && !inputFocus"]
    },
    {
      "before": ["leader", "l", "c"],
      "command": "typescript.restartTsServer"
    },
    {
      "before": ["leader", "`"],
      "commands": ["workbench.action.previousEditor"]
    },
    {
      "before": ["leader", "leader"],
      "commands": ["workbench.action.nextEditor"]
    },
    {
      "before": ["leader", "s", "i"],
      "commands": ["workbench.action.gotoSymbol"]
    },
    {
      "before": ["leader", "o"],
      "commands": ["workbench.action.revealInExplorer"],
      "when": ["editorTextFocus && !sideBarFocus"]
    },
    {
      "key": "gd",
      "command": "editor.action.revealDefinition",
      "when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor"
    },
    {
      "before": ["ctrl+i"],
      "commands": ["editor.action.goToImplementation"],
      "when": [
        "editorHasImplementationProvider && editorTextFocus && !isInEmbeddedEditor"
      ]
    },
    {
      "before": ["leader", "b", "b"],
      "commands": ["workbench.action.showAllEditorsByMostRecentlyUsed"]
    }
  ],
  "editor.inlineSuggest.enabled": true,
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["p"],
      "commands": ["editor.action.clipboardPasteAction"]
    },
    {
      "before": [">"],
      "commands": ["editor.action.indentLines"]
    },
    {
      "before": ["<"],
      "commands": ["editor.action.outdentLines"]
    }
  ],
  "settings.cycle": [
    {
      "id": "copilot",
      "overrideWorkspaceSettings": true,
      "values": [
        {
          "github.copilot.inlineSuggest.enable": false
        },
        {
          "github.copilot.inlineSuggest.enable": true
        }
      ]
    },
    {
      "id": "errorLens",
      "overrideWorkspaceSettings": true,
      "values": [
        {
          "ErrorLens.enable": true
        },
        {
          "ErrorLens.disable": true
        }
      ]
    }
  ],
  "github.copilot.inlineSuggest.enable": true,
  "[python]": {
    "editor.formatOnType": true
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cSpell.userWords": [
    "aframe",
    "argparser",
    "Authed",
    "AVAILABLED",
    "BaÃ±os",
    "blacknound",
    "Catppuccin",
    "clasificados",
    "cloudfare",
    "cooldown",
    "coqui",
    "coquimono",
    "defaultnew",
    "easymotion",
    "esbenp",
    "gigachad",
    "hlsearch",
    "homefinder",
    "hrefs",
    "incsearch",
    "lxml",
    "macmaker",
    "mantine",
    "mooon",
    "mypy",
    "nextjs",
    "nodekeys",
    "Nound",
    "ONNX",
    "outdent",
    "Pepito",
    "precission",
    "pycache",
    "radisk",
    "rana",
    "ranamono",
    "Ranita",
    "Ratelimit",
    "ratelimiter",
    "rgba",
    "rindexed",
    "sqft",
    "superjson",
    "tabler",
    "Tahoma",
    "Tensorflowjs",
    "testid",
    "TRPC",
    "typesafety",
    "upstash",
    "VRAM",
    "webgpu",
    "webrtc"
  ],
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "errorLens.enabledDiagnosticLevels": ["warning", "info"],
  "workbench.colorTheme": "Naysayer88",
  "eslint.packageManager": "pnpm",
  "errorLens.messageBackgroundMode": "none",
  "eslint.rules.customizations": [{ "rule": "*", "severity": "warn" }],
  "workbench.colorCustomizations": {
    "editorError.foreground": "#00000000",
    "editorWarning.foreground": "#00000000",
    "editorInfo.foreground": "#00000000"
  },
  "workbench.activityBar.visible": false
}
