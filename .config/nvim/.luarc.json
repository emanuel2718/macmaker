{
  "$schema": "https://raw.githubusercontent.com/sumneko/vscode-lua/master/setting/schema.json",
  "Lua.workspace.checkThirdParty": false,
  "disable": [
    "close-non-object",
    "incomplete-signature-doc",
    "missing-global-doc",
    "missing-local-export-doc",
    "missing-fields"
  ],
  "groupFileStatus": {
    "ambiguity": "Any",
    "await": "Any",
    "duplicate": "Any",
    "global": "Any",
    "luadoc": "Any",
    "redefined": "Any",
    "strict": "Any",
    "type-check": "Any",
    "unbalanced": "Any",
    "unused": "Any"
  },
  "ignoredFiles": "Opened",
  "libraryFiles": "Opened",
  "diagnostics.globals": [
    "vim"
  ]
}